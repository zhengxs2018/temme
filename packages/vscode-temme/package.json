{
  "name": "vscode-temme",
  "private": true,
  "license": "MIT",
  "displayName": "Temme",
  "description": "Temme selector support for VSCode.",
  "version": "0.8.3",
  "publisher": "shinima",
  "repository": "https://github.com/shinima/temme.git",
  "main": "dist/index.js",
  "scripts": {},
  "vsce": {
    "dependencies": true
  },
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "languages": [
      {
        "id": "temme",
        "aliases": [
          "temme",
          "temme"
        ],
        "extensions": [
          ".temme"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "temme",
        "scopeName": "source.temme",
        "path": "./dist/temme.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "temme.runSelector",
        "category": "Temme",
        "title": "Run temme selector"
      },
      {
        "command": "temme.startWatch",
        "category": "Temme",
        "title": "Start watching"
      },
      {
        "command": "temme.stop",
        "category": "Temme",
        "title": "Stop watching"
      }
    ]
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "temme": "workspace:^"
  },
  "devDependencies": {
    "js-yaml": "^4.1.0",
    "vscode": "1.1.36"
  }
}
